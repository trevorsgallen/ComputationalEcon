set more off

clear
quietly infix                ///
  int     year        1-4    ///
  long    serial      5-9    ///
  float   hwtsupp     10-19  ///
  byte    asecflag    20-20  ///
  byte    month       21-22  ///
  byte    pernum      23-24  ///
  float   wtsupp      25-34  ///
  int     relate      35-38  ///
  byte    age         39-40  ///
  byte    wkswork1    41-42  ///
  int     uhrsworkly  43-45  ///
  double  ftotval     46-55  ///
  long    incwage     56-62  ///
  using `"/users/tgallen/Dropbox/Econ_641/Fall_2016/Homework_TaxFit/cps_00233.dat"'

replace hwtsupp    = hwtsupp    / 10000
replace wtsupp     = wtsupp     / 10000

format hwtsupp    %10.4f
format wtsupp     %10.4f
format ftotval    %10.0f

