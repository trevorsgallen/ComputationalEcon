@article{GallenMulligan,
	abstract = {The Affordable Care Act's taxes, subsidies, and regulations significantly alter terms of trade in both goods and factor markets. We use an extended version of the classic Harberger model to predict and quantify consequences of the Affordable Care Act for the incidence of health insurance coverage and patterns of labor usage. If and when the new exchange plans are competitive with employer-sponsored insurance (ESI), our model predicts that more than 22 million people will leave ESI as a consequence of the law. Behavioral changes are expected to add two million participants to the new exchange plans: beyond those that would participate solely as the result of employer decisions to stop offering coverage and beyond those who would have been uninsured. We find large differences in coverage-pattern impacts based on the benefit (including tax incentives) of joining exchange plans and degree to which statutory penalties on individuals and firms are implemented: If exchange plans were not valued while the individual mandate were fully enforced, ESI could potentially even expand.},
	author = {Trevor S. Gallen and Casey B. Mulligan},
	journal = {National Tax Journal},
	month = {March},
	number = {1},
	pages = {75-120},
	title = {{Wedges, Labor Market Behavior, and Health Insurance Coverage under the Affordable Care Act}},
	url = {https://ideas.repec.org/a/ntj/journl/v71y2018i1p75-120.html},
	volume = {71},
	year = 2018,
	Bdsk-Url-1 = {https://ideas.repec.org/a/ntj/journl/v71y2018i1p75-120.html}}
