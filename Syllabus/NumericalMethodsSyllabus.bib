	publisher = {[Economics Department of the University of Pennsylvania, Wiley, Institute of Social and Economic Research, Osaka University]},
	title = {A Structural Model of Multiple Welfare Program Participation and Labor Supply},
	url = {http://www.jstor.org/stable/2527390},
	volume = {39},
	year = {1998},
	Bdsk-Url-1 = {http://www.jstor.org/stable/2527390}}

@article{doi:10.3982/ECTA7925,
	abstract = {Estimating structural models is often viewed as computationally difficult, an impression partly due to a focus on the nested fixed-point (NFXP) approach. We propose a new constrained optimization approach for structural estimation. We show that our approach and the NFXP algorithm solve the same estimation problem, and yield the same estimates. Computationally, our approach can have speed advantages because we do not repeatedly solve the structural equation at each guess of structural parameters. Monte Carlo experiments on the canonical Zurcher bus-repair model demonstrate that the constrained optimization approach can be significantly faster.},
	author = {Su, Che-Lin and Judd, Kenneth L.},
	doi = {10.3982/ECTA7925},
	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.3982/ECTA7925},
	journal = {Econometrica},
	keywords = {Structural estimation, dynamic discrete choice models, constrained optimization},
	number = {5},
	pages = {2213-2230},
	title = {Constrained Optimization Approaches to Estimation of Structural Models},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.3982/ECTA7925},
